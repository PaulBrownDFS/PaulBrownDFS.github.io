<div class="lightSliderContainer {{sliderIdName}}" data-desktop="{{showDesktop}}" data-mobile="{{showMobile}}">
  <div class="item">
    <div id="content-slider" class="content-slider">
      {{#each slides}}
      <div class="slide{{@index}}" style="position:relative">
        {{#if imageLink}}
        <a href="{{imageLink}}">
        {{/if}}
        <picture>
        {{#if bannerImage}}
        <!-- WebP Banner Image Type -->
          <!--600 Break Point-->
          <source type="image/webp" media="(max-width: 1023px)"  srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}_mobile{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=600" alt="{{bannerImage.[0].imageAltText}}">

          <!--1366 Break Point-->
          <source type="image/webp" media="(min-width: 1366px)"
          srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=1366" alt="{{bannerImage.[0].imageAltText}}">

          <!--1024 Break Point -->
          <source type="image/webp" media="(max-width: 1365px)" srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=1024" alt="{{bannerImage.[0].imageAltText}}">

          <!--Fallback BannerImage-->
          <img src="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=1306" alt="{{bannerImage.[0].imageAltText}}">

          <!-- Jpeg Banner Image Type -->
            <!--600 Break Point-->
            <source type="image/jpeg" media="(max-width: 1023px)"  srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}_mobile{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=600" alt="{{bannerImage.[0].imageAltText}}">

            <!--1366 Break Point-->
            <source type="image/jpeg" media="(min-width: 1366px)"
            srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=1366" alt="{{bannerImage.[0].imageAltText}}">

            <!--1024 Break Point -->
            <source type="image/jpeg" media="(max-width: 1365px)" srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=1024" alt="{{bannerImage.[0].imageAltText}}">

            <!--Fallback BannerImage-->
            <img src="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=1306" alt="{{bannerImage.[0].imageAltText}}">
        {{else}}
          <!-- WebP Image Type -->
          <!--600 Break Point -->
        {{#if ../showMobile}}
          <source type="image/webp" media="(max-width: 1023px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if ../sliderVersion}}_{{../sliderVersion}}{{/if}}_mobile{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp{{#if imageAttributesMob}}?{{imageAttributesMob}}{{else}}?w=600{{/if}}" alt="{{imageAltText}}">
        {{/if}}

          <!--1366 Break Point -->
        {{#if ../showDesktop}}
          <source type="image/webp" media="(min-width: 1366px)"
          srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if ../sliderVersion}}_{{../sliderVersion}}{{/if}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp{{#if imageAttributes}}?{{imageAttributes}}{{else}}?w=1366{{/if}}" alt="{{imageAltText}}">


          <!--1024 Break Point -->
          <source type="image/webp" media="(max-width: 1365px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if ../sliderVersion}}_{{../sliderVersion}}{{/if}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp{{#if imageAttributes24}}?{{imageAttributes24}}{{else}}?w=1024{{/if}}" alt="{{imageAltText}}">
        {{/if}}

          <!-- Jpeg Image Type -->
          <!--600 Break Point -->
        {{#if ../showMobile }}
          <source media="(max-width: 1023px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if ../sliderVersion}}_{{../sliderVersion}}{{/if}}_mobile{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributesMob}}?{{imageAttributesMob}}{{else}}?w=600{{/if}}" alt="{{imageAltText}}">
        {{/if}}

          <!--1366 Break Point -->
        {{#if ../showDesktop}}
          <source media="(min-width: 1366px)"
          srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if ../sliderVersion}}_{{../sliderVersion}}{{/if}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributes}}?{{imageAttributes}}{{else}}?w=1366{{/if}}" alt="{{imageAltText}}">

          <!--1024 Break Point -->
          <source media="(max-width: 1365px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if ../sliderVersion}}_{{../sliderVersion}}{{/if}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributes24}}?{{imageAttributes24}}{{else}}?w=1024{{/if}}" alt="{{imageAltText}}">
        {{/if}}

          <!--Fallback Image-->
          <img src="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if ../sliderVersion}}_{{../sliderVersion}}{{/if}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributes}}?{{imageAttributes}}{{else}}{{#if ../showDesktop}}?w=1366{{else}}?w=600{{/if}}{{/if}}" alt="{{imageAltText}}">

        {{/if}}
        </picture>
        {{#if imageLink}}
          </a>
        {{/if}}


        {{#each roundel}}
          <div class="RoundelWrapper">
            <picture class="amp-dc-roundel roundelID{{@index}} {{deviceList}}">
              {{#if roundelLink}}
              <a title="{{{roundelToolTip}}}" href="{{roundelLink}}">
              {{/if}}
                <img src="//{{roundel.defaultHost}}/i/{{roundel.endpoint}}/{{escapeUrl roundel.name}}{{#if ../imageVersion}}_{{../imageVersion}}{{/if}}" alt="{{{roundelToolTip}}}">
              {{#if roundelLink}}
              </a>
              {{/if}}
            </picture>
          </div>
          <style>
        {{#if ../../showDesktop}}
          .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}} {
            position: absolute;
            {{roundelAnchor.[0]}}: {{replace d1366Position.[1] 'y=' ''}}px;
            {{roundelAnchor.[1]}}: {{replace d1366Position.[0] 'x=' ''}}px;
          }
          .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.UK.Desktop {
            display:block;
          }
          .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.ROI.Desktop {
            display:block;
          }
          .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.NL.Desktop {
            display:block;
          }
          .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.ES.Desktop {
            display:block;
          }

          @media (max-width: 1365px) {

            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}} {
              {{roundelAnchor.[0]}}: {{replace d1024Position.[1] 'y=' ''}}px;
              {{roundelAnchor.[1]}}: {{replace d1024Position.[0] 'x=' ''}}px;
            }

          }
        {{/if}}
        {{#if ../../showMobile}}
          @media (max-width: 1023px) {
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}} {
              {{roundelAnchor.[0]}}: {{replace mobilePosition.[1] 'y=' ''}}px;
              {{roundelAnchor.[1]}}: {{replace mobilePosition.[0] 'x=' ''}}px;
            }

            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.UK.Mobile {
              display:block !important;
            }
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.ROI.Mobile {
              display:block !important;
            }
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.NL.Mobile {
              display:block !important;
            }
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.ES.Mobile {
              display:block !important;
            }
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.UK.Desktop {
              display:none;
            }
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.ROI.Desktop {
              display:none;
            }
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.NL.Desktop {
              display:none;
            }
            .{{../../sliderIdName}} .amp-dc-roundel.roundelID{{@index}}.ES.Desktop {
              display:none;
            }

          }
        {{/if}}
          </style>
        {{/each}}

      </div>

  {{/each}}

    </div>
  </div>
</div>
<style>
{{#if showDesktop}}
  .js_slider_wrap {
      position: relative;
  }
  .lightSliderContainer.{{sliderIdName}} {
    width: {{imageWidth}}px;
    margin:0 auto;
  }
  @media (max-width: 1365px) {
    .lightSliderContainer.{{sliderIdName}} {
      width: {{imageWidth24}}px;
    }
  }
{{/if}}
  {{#if showMobile}}
  @media (max-width: 1023px) {
    .lightSliderContainer.{{sliderIdName}}{
      width: {{imageWidthMob}}px;
    }
  }
  {{/if}}
</style>
