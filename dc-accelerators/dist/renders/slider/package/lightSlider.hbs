<div class="lightSliderContainer {{sliderIdName}}">
  <div class="item">
    <div id="content-slider" class="content-slider">
      {{#each slides}}
      <div class="slide{{@index}}">
        <picture>
        {{#if bannerImage}}
        <!-- WebP Banner Image Type -->
          <!--600 Break Point-->
          <source type="image/webp" media="(max-width: 1023px)"  srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}_mobile{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=600" alt="{{bannerImage.[0].imageAltText}}">

          <!--1366 Break Point-->
          <source type="image/webp" media="(min-width: 1366px)"
          srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=1366" alt="{{bannerImage.[0].imageAltText}}">

          <!--1024 Break Point -->
          <source type="image/webp" media="(max-width: 1365px)" srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=1024" alt="{{bannerImage.[0].imageAltText}}">

          <!--Fallback BannerImage-->
          <img src="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.webp?w=1306" alt="{{bannerImage.[0].imageAltText}}">

          <!-- Jpeg Banner Image Type -->
            <!--600 Break Point-->
            <source type="image/jpeg" media="(max-width: 1023px)"  srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}_mobile{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=600" alt="{{bannerImage.[0].imageAltText}}">

            <!--1366 Break Point-->
            <source type="image/jpeg" media="(min-width: 1366px)"
            srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=1366" alt="{{bannerImage.[0].imageAltText}}">

            <!--1024 Break Point -->
            <source type="image/jpeg" media="(max-width: 1365px)" srcset="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=1024" alt="{{bannerImage.[0].imageAltText}}">

            <!--Fallback BannerImage-->
            <img src="//{{bannerImage.[0].image.defaultHost}}/i/{{bannerImage.[0].image.endpoint}}/{{escapeUrl bannerImage.[0].image.name}}{{#if bannerImage.[0].seoText}}/{{escapeUrl bannerImage.[0].seoText}}{{/if}}.jpg?w=1306" alt="{{bannerImage.[0].imageAltText}}">
        {{else}}
          <!-- WebP Image Type -->
          <!--600 Break Point -->
          <source type="image/webp" media="(max-width: 1023px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}_mobile{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp{{#if imageAttributesMob}}?{{imageAttributesMob}}{{else}}?w=600{{/if}}" alt="{{imageAltText}}">

          <!--1366 Break Point -->
          <source type="image/webp" media="(min-width: 1366px)"
          srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp{{#if imageAttributes}}?{{imageAttributes}}{{else}}?w=1366{{/if}}" alt="{{imageAltText}}">

          <!--1024 Break Point -->
          <source type="image/webp" media="(max-width: 1365px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.webp{{#if imageAttributes24}}?{{imageAttributes24}}{{else}}?w=1024{{/if}}" alt="{{imageAltText}}">

          <!-- Jpeg Image Type -->
          <!--600 Break Point -->
          <source media="(max-width: 1023px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}_mobile{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributesMob}}?{{imageAttributesMob}}{{else}}?w=600{{/if}}" alt="{{imageAltText}}">

          <!--1366 Break Point -->
          <source media="(min-width: 1366px)"
          srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributes}}?{{imageAttributes}}{{else}}?w=1366{{/if}}" alt="{{imageAltText}}">

          <!--1024 Break Point -->
          <source media="(max-width: 1365px)" srcset="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributes24}}?{{imageAttributes24}}{{else}}?w=1024{{/if}}" alt="{{imageAltText}}">

          <!--Fallback Image-->
          <img src="//{{image.defaultHost}}/i/{{image.endpoint}}/{{escapeUrl image.name}}{{#if seoText}}/{{escapeUrl seoText}}{{/if}}.jpg{{#if imageAttributes}}?{{imageAttributes}}{{else}}?w=1366{{/if}}" alt="{{imageAltText}}">
        </picture>
        {{/if}}

      </div>
      {{/each}}

    </div>
  </div>
</div>
